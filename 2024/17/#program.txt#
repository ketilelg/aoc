2,4,
1,3,
7,5,
1,5,
0,3,
4,3,
5,5,
3,0

a ??
b 0
c 0

bst 4 # b=a%8	 - b in range 0-7
bxl 3 # b=b^3 	 - b in range 0-7
cdv 5 # c=a/2**b - divisor: range 1-128
bxl 1 # b b=b^1  - b shift odd/even
adv 3 # a=a/8 	 - a/8
bxc 3 # b=b^c	 - simplify: b=b^(a/2**b)
out 5 # reg b%8  - b mod 8
jnz 0 # jump if a!=0 
